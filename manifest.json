{
    "manifest_version": 2,
    "name": "CORS",
    "version": "1.0.0",
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "19": "images/off.png"
        },
        "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "<all_urls>"
    ]

}
